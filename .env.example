# Environment variables for Supabase integration
# Copy this file to .env and fill in your Supabase credentials

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# Flask Configuration
FLASK_ENV=development
SECRET_KEY=your_secret_key_here
# 5GB max file size
MAX_CONTENT_LENGTH=5368709120

# Storage Configuration
SUPABASE_BUCKET_UPLOADS=uploads
SUPABASE_BUCKET_OUTPUTS=outputs
SUPABASE_BUCKET_SPLITS=splits

# Database Configuration
DATABASE_URL=your_supabase_database_url_here
